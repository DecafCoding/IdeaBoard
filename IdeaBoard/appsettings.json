{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Supabase": {
    "Url": "YOUR_SUPABASE_URL",
    "AnonKey": "YOUR_SUPABASE_ANON_KEY"
  },
  "Canvas": {
    "AutoSaveDebounceMs": 1000,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000,
    "MaxItemsPerBoard": 500,
    "DefaultNoteSize": {
      "Width": 200,
      "Height": 150
    },
    "MinZoom": 0.1,
    "MaxZoom": 3.0
  },
  "Storage": {
    "ImageBucket": "canvas-images",
    "MaxImageSizeBytes": 5242880,
    "AllowedImageTypes": [
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/webp"
    ]
  }
}
